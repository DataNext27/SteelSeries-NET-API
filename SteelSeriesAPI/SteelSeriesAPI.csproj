<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>net6.0;net7.0;net8.0</TargetFrameworks>
        <Version>1.0.0</Version>
        <Authors>DataNext</Authors>
        <PackageProjectUrl>https://github.com/datanext27</PackageProjectUrl>
        <RepositoryUrl>https://github.com/datanext27.git</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <Title>SteelSeries .NET API</Title>
        <Description>Unofficial .NET SteelSeries API</Description>
        <OutputType>Library</OutputType>
        <AssemblyName>SteelSeriesAPI</AssemblyName>
        <RootNamespace>SteelSeriesAPI</RootNamespace>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>

</Project>
